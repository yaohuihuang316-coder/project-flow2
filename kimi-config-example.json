{
  "_comment": "将此配置复制到 ~/.kimi/config.json",
  "mcpServers": {
    "vercel": {
      "command": "npx",
      "args": ["-y", "@vercel/mcp@latest"],
      "env": {
        "VERCEL_TOKEN": "YOUR_VERCEL_TOKEN_HERE"
      }
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@github/mcp-server-github@latest"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "YOUR_GITHUB_TOKEN_HERE"
      }
    }
  }
}
